<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SDNotifyRadioButton">
        <attr name="sd_notify_color" format="color" />
        <attr name="sd_notify_radius" format="dimension" />
        <attr name="sd_notify_show" format="boolean" />
    </declare-styleable>
    <!--<attr name="sd_notify_color" format="color" />-->
    <!--<attr name="sd_notify_radius" format="dimension" />-->
    <!--<attr name="sd_notify_show" format="boolean" />-->

    <!--Line EditText-->
    <declare-styleable name="SDLineEditText">
        <attr name="line_color" />
    </declare-styleable>
    <attr name="line_color" format="color" />
    <!--StepView-->
    <declare-styleable name="SDStepView">
        <attr name="outerColor" />
        <attr name="innerColor" />
        <attr name="borderWidth" />
        <attr name="stepViewTextSize" />
        <attr name="stepViewTextColor" />
    </declare-styleable>
    <attr name="outerColor" format="color" />
    <attr name="innerColor" format="color" />
    <attr name="borderWidth" format="dimension" />
    <attr name="stepViewTextSize" format="dimension" />
    <attr name="stepViewTextColor" format="color" />

    <!--@Time：2017-09-04——Start-->
    <declare-styleable name="RoundView">
        <attr name="r_backgroundColor" />
        <attr name="r_backgroundPressColor" />
        <attr name="r_cornerRadius" />
        <attr name="r_strokeWidth" />
        <attr name="r_strokeColor" />
        <attr name="r_strokePressColor" />
        <attr name="r_textPressColor" />
        <attr name="r_isRadiusHalfHeight" />
        <attr name="r_isWidthHeightEqual" />
        <attr name="r_cornerRadius_TL" />
        <attr name="r_cornerRadius_TR" />
        <attr name="r_cornerRadius_BL" />
        <attr name="r_cornerRadius_BR" />
        <attr name="r_isRippleEnable" />
    </declare-styleable>
    <!-- 圆角矩形背景色 -->
    <attr name="r_backgroundColor" format="color" />
    <!-- 圆角矩形背景色press -->
    <attr name="r_backgroundPressColor" format="color" />
    <!-- 圆角弧度,单位dp-->
    <attr name="r_cornerRadius" format="dimension" />
    <!-- 圆角弧度,单位dp-->
    <attr name="r_strokeWidth" format="dimension" />
    <!-- 圆角边框颜色-->
    <attr name="r_strokeColor" format="color" />
    <!-- 圆角边框颜色press -->
    <attr name="r_strokePressColor" format="color" />
    <!-- 文字颜色press-->
    <attr name="r_textPressColor" format="color" />
    <!-- 圆角弧度是高度一半-->
    <attr name="r_isRadiusHalfHeight" format="boolean" />
    <!-- 圆角矩形宽高相等,取较宽高中大值-->
    <attr name="r_isWidthHeightEqual" format="boolean" />
    <!-- 圆角弧度,单位dp,TopLeft-->
    <attr name="r_cornerRadius_TL" format="dimension" />
    <!-- 圆角弧度,单位dp,TopRight-->
    <attr name="r_cornerRadius_TR" format="dimension" />
    <!-- 圆角弧度,单位dp,BottomLeft-->
    <attr name="r_cornerRadius_BL" format="dimension" />
    <!-- 圆角弧度,单位dp,BottomRight-->
    <attr name="r_cornerRadius_BR" format="dimension" />
    <!-- 是否有Ripple效果,api21+有效-->
    <attr name="r_isRippleEnable" format="boolean" />
    <!--@Time：2017-09-04——End-->
    <!--TabRadioButton-->
    <declare-styleable name="SDDrawableRadioButton">
        <attr name="sd_drawableSize" format="dimension"  />
        <attr name="sd_drawableTop" format="reference"/>
        <attr name="sd_drawableLeft" format="reference"/>
        <attr name="sd_drawableRight" format="reference"/>
        <attr name="sd_drawableBottom" format="reference"/>
    </declare-styleable>
    <!--<attr name="drawableSize" format="dimension" />-->
    <!--<attr name="drawableTop" format="reference" />-->
    <!--<attr name="drawableLeft" format="reference" />-->
    <!--<attr name="drawableRight" format="reference" />-->
    <!--<attr name="drawableBottom" format="reference" />-->
    <!--@Time：2017-09-05——Start-->
    <declare-styleable name="SDHexagonView">
        <attr name="h_radius" />
        <attr name="h_outerWidth" />
        <attr name="h_outerColor" />
        <attr name="h_innerColor" />
        <attr name="h_isHasStroke" />
    </declare-styleable>
    <attr name="h_radius" format="dimension|reference" />
    <attr name="h_outerWidth" format="dimension|reference" />
    <attr name="h_outerColor" format="color|reference" />
    <attr name="h_innerColor" format="color|reference" />
    <attr name="h_isHasStroke" format="boolean|reference" />
    <!--@Time：2017-09-05——End-->
    <!--@Tme:2017-10-02——Start-->
    <declare-styleable name="SDRatingBar">
        <!--默认星星图片 -->
        <attr name="sd_normalStar" />
        <!--选中星星图片 -->
        <attr name="sd_selectStar" />
        <!--星星的总数量 -->
        <attr name="sd_starNum" />
        <!--星星的间隔 -->
        <attr name="sd_starPadding" />
    </declare-styleable>
    <attr name="sd_normalStar" format="reference" />
    <attr name="sd_selectStar" format="reference" />
    <attr name="sd_starNum" format="integer" />
    <attr name="sd_starPadding" format="dimension" />
    <!--@Tme:2017-10-02——End-->
    <!--@Tme:2017-10-03——Start-->
    <declare-styleable name="SDLetterIndexView">
        <!--字母文字颜色-->
        <attr name="sd_defaultTextColor" />
        <!--选中时字母文字颜色-->
        <attr name="sd_selectTextColor" />
        <!--字母文字大小-->
        <attr name="sd_letterTextSize" />
    </declare-styleable>
    <attr name="sd_defaultTextColor" format="color" />
    <attr name="sd_selectTextColor" format="color" />
    <attr name="sd_letterTextSize" format="dimension" />
    <!--@Tme:2017-10-03——End-->
    <!--2017-12-23-->
    <!-- RTextView -->
    <declare-styleable name="RTextView">

        <!--四周圆角度、左上、右上、左下、右下-->
        <attr name="sd_corner_radius" format="dimension" />
        <attr name="sd_corner_radius_top_left" format="dimension" />
        <attr name="sd_corner_radius_top_right" format="dimension" />
        <attr name="sd_corner_radius_bottom_left" format="dimension" />
        <attr name="sd_corner_radius_bottom_right" format="dimension" />

        <!--虚线边框宽度/间隔-->
        <attr name="sd_border_dash_width" format="dimension" />
        <attr name="sd_border_dash_gap" format="dimension" />

        <!--边框宽度-->
        <attr name="sd_border_width_normal" format="dimension" />
        <attr name="sd_border_width_pressed" format="dimension" />
        <attr name="sd_border_width_unable" format="dimension" />

        <!--边框颜色-->
        <attr name="sd_border_color_normal" format="color" />
        <attr name="sd_border_color_pressed" format="color" />
        <attr name="sd_border_color_unable" format="color" />

        <!--背景颜色-->
        <attr name="sd_background_normal" format="color" />
        <attr name="sd_background_pressed" format="color" />
        <attr name="sd_background_unable" format="color" />

        <!--文本颜色-->
        <attr name="sd_text_color_normal" format="color" />
        <attr name="sd_text_color_pressed" format="color" />
        <attr name="sd_text_color_unable" format="color" />

        <!--图标资源-->
        <attr name="sd_icon_src_normal" format="reference" />
        <attr name="sd_icon_src_pressed" format="reference" />
        <attr name="sd_icon_src_unable" format="reference" />

        <!--图标、大小、位置-->
        <attr name="sd_icon_height" format="dimension" />
        <attr name="sd_icon_width" format="dimension" />
        <attr name="sd_icon_direction">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <!--字体样式-->
        <attr name="sd_text_typeface" format="string" />
    </declare-styleable>
    <declare-styleable name="SDAvatarListLayout">
        <!--创建的最大ImageView数量-->
        <attr name="image_max_count" format="integer" />
        <!--创建的ImageView的大小-->
        <attr name="image_size" format="dimension" />
        <!--要叠加的ImageView偏移量-->
        <attr name="image_offset" format="float" />
    </declare-styleable>
    <declare-styleable name="SDCircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
</resources>